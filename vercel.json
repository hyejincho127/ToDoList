{
    "version": 2,
    "builds": [
      {
        "src": "api/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "api/json-server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/db", "dest": "json-server.js" },
      { "src": "/api/(.*)", "dest": "api/$1" }
    ]
  }
  