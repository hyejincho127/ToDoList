{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/json-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/db",
      "dest": "/api/json-server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
